{
  "name": "claudedesk",
  "version": "3.4.2",
  "description": "AI-powered development platform with CLI and web interface for Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "claudedesk": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "config/*.example.json",
    "config/skills/*.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:alt": "cross-env CLAUDEDESK_PORT=9787 VITE_DEV_PORT=5174 npm run dev",
    "dev:server": "tsx watch src/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc",
    "build:client": "vite build",
    "start": "node dist/index.js",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "development",
    "cli",
    "terminal",
    "session-manager"
  ],
  "author": "ClaudeDesk Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/carloluisito/claudedesk.git"
  },
  "bugs": {
    "url": "https://github.com/carloluisito/claudedesk/issues"
  },
  "homepage": "https://github.com/carloluisito/claudedesk#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/glob": "^8.1.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "express": "^4.21.0",
    "framer-motion": "^11.5.0",
    "get-port": "^7.1.0",
    "glob": "^13.0.0",
    "lucide-react": "^0.441.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "playwright": "^1.49.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.21.0",
    "tailwind-merge": "^2.5.0",
    "tree-kill": "^1.2.2",
    "uuid": "^11.0.0",
    "ws": "^8.19.0",
    "yaml": "^2.4.0",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  }
}
